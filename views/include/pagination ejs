<div class="card">
                        <div class="card-inner">
                            <div>
                                <ul class="pagination">
                                    <% if (currentPage> 1) { %>
                                        <li class="page-item"><a class="page-link" href="<%= prevPageUrl %>">Prev</a>
                                        </li>
                                        <% } %>

                                            <% for (let i=1; i <=totalPages; i++) { %>
                                                <% if (i===currentPage) { %>
                                                    <li class="page-item active"><span class="page-link">
                                                            <%= i %>
                                                        </span></li>
                                                    <% } else { %>
                                                        <li class="page-item"><a class="page-link"
                                                                href="<%= pageUrl(i) %>">
                                                                <%= i %>
                                                            </a></li>
                                                        <% } %>
                                                            <% } %>

                                                                <% if (currentPage < totalPages) { %>
                                                                    <li class="page-item"><a class="page-link"
                                                                            href="<%= nextPageUrl %>">Next</a></li>
                                                                    <% } %>
                                </ul>
                            </div>
                        </div>

                    </div>