<!-- Updated jQuery code for populating dropdowns -->
<script>
  $(document).ready(function () {
    // Fetch brands from the API
    $.get("http://localhost:3000/attributes/brands", function (data) {
      var dropdown = $("#brandDropdown");

      // Populate the dropdown with brands
      data.forEach(function (brand) {
        dropdown.append(new Option(brand.name, brand._id));
      });
    });
  });

  $(document).ready(function () {
    // Function to load subcategories based on the given category
    var loadSubcategories = function (category) {
      $.get("http://localhost:3000/attributes/category/" + category, function (data) {
        var subcategoryDropdown = $("#subcategoryDropdown");

        // Clear existing options
        subcategoryDropdown.empty();

        // Add a default option
        subcategoryDropdown.append(new Option("Select a subcategory", ""));

        // Populate the subcategory dropdown
        data.forEach(function (item) {
          subcategoryDropdown.append(new Option(item.Subcategory, item._id));
        });
      });
    };

    // Event handler for when category dropdown changes
    $("#categoryDropdown").change(function () {
      var selectedCategory = $(this).val();
      loadSubcategories(selectedCategory);
    });

    // Populate subcategories for Men by default on page load
    loadSubcategories("Men");
  });
</script>





